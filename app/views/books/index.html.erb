<h1>Books</h1>
<tbody>
 <tr><%= @books.each do |book| %></tr>
  <th><%= book.title %></th>
  <th><%= book.body %></th>
 </tr>
</tbody>

<% end %>

<h2>New Book</h2>
<%= form_with model: Book.new do |f| %>

<h5 = "title">Title</h5>
<%= f.text_field :title %>

<h5 ="Body">Body</h5>
<%= f.text_area :body %>

<%= f.submit 'Create Book' %>
<% end %>